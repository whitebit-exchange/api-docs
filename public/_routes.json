{
  "version": 1,
  "include": ["/*"],
  "exclude": [],
  "routes": [
    {
      "src": "/overview",
      "dst": "/platform/overview",
      "status": 301
    },
    {
      "src": "/colocation",
      "dst": "/platform/colocation",
      "status": 301
    },
    {
      "src": "/oauth",
      "dst": "/platform/oauth",
      "status": 301
    },
    {
      "src": "/oauth/:path*",
      "dst": "/platform/oauth/:path",
      "status": 301
    },
    {
      "src": "/webhook",
      "dst": "/platform/webhook",
      "status": 301
    },
    {
      "src": "/webhook/:path*",
      "dst": "/platform/webhook/:path",
      "status": 301
    }
  ]
}
